{
  "name": "zazu-plugins",
  "version": "1.0.0",
  "description": "various Zazu plugins all in one",
  "blocks": {
    "input": [
      {
        "id": "dict",
        "type": "PrefixScript",
        "prefix": "dc",
        "space": true,
        "args": "optional",
        "script": "src/dict.js",
        "connections": ["QueryDict"]
      },
      {
        "id": "blog-search",
        "type": "PrefixScript",
        "prefix": "s",
        "space": true,
        "args": "Required",
        "script": "src/blog-search.js",
        "connections": ["SearchDraft"]
      },
      {
        "id": "todo",
        "type": "Keyword",
        "keyword": "todo",
        "title": "Open todo list",
        "connections": ["GetTodoURL"]
      },
      {
        "id": "status",
        "type": "Keyword",
        "keyword": "status",
        "title": "Open status page",
        "connections": ["GetStatusURL"]
      },
      {
          "id": "vault",
          "type": "PrefixScript",
          "prefix": "v",
          "space": true,
          "args": "Required",
          "script": "src/vault.js",
          "connections": ["Copy"]
      }
    ],
    "output": [
      {
        "id": "QueryDict",
        "type": "OpenInBrowser",
        "url": "dict://{value}"
      },
      {
        "id": "SearchDraft",
        "type": "OpenInBrowser",
        "url": "{value}"
      },
      {
        "id": "GetTodoURL",
        "type": "UserScript",
        "script": "src/todo.js",
        "connections": ["Browser"]
      },
      {
        "id": "GetStatusURL",
        "type": "UserScript",
        "script": "src/status.js",
        "connections": ["Browser"]
      },
      {
        "id": "Browser",
        "type": "OpenInBrowser",
        "url": "{value}"
      },
      {
        "id": "Copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      }
    ]
  }
}

